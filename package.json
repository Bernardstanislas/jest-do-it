{
  "name": "jest-do-it",
  "version": "0.0.2",
  "description": "Learn to test anything with Jest",
  "main": "jest-do-it.js",
  "bin": {
    "jest-do-it": "./jest-do-it.js"
  },
  "scripts": {
    "test": "standard"
  },
  "standard": {
    "globals": [
      "describe",
      "it",
      "test",
      "expect",
      "jest",
      "fetch",
      "afterEach"
    ]
  },
  "jest": {
    "testMatch": [
      "**/*.js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/exercises/",
      "metatest"
    ]
  },
  "keywords": [
    "nodeschool",
    "workshopper",
    "learn",
    "cli",
    "tap"
  ],
  "author": "Stanislas Bernard",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bernardstanislas/jest-do-it/issues"
  },
  "homepage": "https://github.com/bernardstanislas/jest-do-it",
  "dependencies": {
    "adventure": "^2.11.0",
    "adventure-verify": "^2.2.0",
    "cli-md": "^1.0.0",
    "jest": "^20.0.4",
    "workshopper-exercise": "^2.3.0"
  },
  "preferGlobal": true,
  "devDependencies": {
    "standard": "^8.5.0"
  }
}
